    <!-- http://www.semanticweb.org/helenamontenegro/ontologies/2020/11/untitled-ontology-8#Q20817252 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/helenamontenegro/ontologies/2020/11/untitled-ontology-8#Q20817252">
