        <hasCauses rdf:resource="http://www.semanticweb.org/helenamontenegro/ontologies/2020/11/untitled-ontology-8#Q701609"/>
        <hasSpecialty rdf:resource="http://www.semanticweb.org/helenamontenegro/ontologies/2020/11/untitled-ontology-8#Q898674"/>
